<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:key="instant_upload_local_path"
        android:persistent="false"
        android:title="@string/prefs_instant_upload_local_path_title"/>
    <EditTextPreference
        android:key="instant_upload_remote_path"
        android:persistent="false"
        android:title="@string/prefs_instant_upload_remote_path_title"/>
    <com.owncloud.android.ui.CheckBoxPreferenceWithLongTitle
        android:key="instant_upload_on_wifi"
        android:persistent="false"
        android:title="@string/prefs_instant_upload_only_wifi"/>
    <com.owncloud.android.ui.CheckBoxPreferenceWithLongTitle
        android:key="instant_video_upload_on_charging"
        android:persistent="false"
        android:title="@string/prefs_instant_upload_only_charging"/>
</PreferenceScreen>
