<?xml version="1.0" encoding="UTF-8"?>


<FindBugsFilter>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>

    <!-- Dagger code is autogenerated. Exclude it from Check. -->
    <Match>
        <Or>
            <Class name="~.*\.Dagger.*"/>
            <Class name="~com.nextcloud.client.di\..*_.*"/>
        </Or>

    </Match>
    <Bug pattern="PATH_TRAVERSAL_IN" />
    <Bug pattern="ANDROID_EXTERNAL_FILE_ACCESS" />
</FindBugsFilter>
